{
  "expo": {
    "name": "StyleAI",
    "slug": "styleai-virtual-tryon",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "styleai",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "beb458fe-db70-4713-bd6f-bbd6fc13ea17"
      }
      },
    "android": {
      "package": "com.shivamguptaa.styleaivirtualtryon"
    }
  }
}
